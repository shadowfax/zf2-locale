{
    "name": "unjudder/locale",
    "description": "Automatic detection of locales for Zend Framework 2",
    "type": "library",
    "keywords": [
        "zf2",
        "i18n",
        "locale"
    ],
    "homepage": "https://github.com/juriansluiman/SlmLocale",
    "authors": [
        {
            "name": "Jurian Sluiman",
            "email": "jurian@soflomo.com",
            "homepage": "http://juriansluiman.nl"
        },
        {
            "name": "Alrik Zachert",
            "email": "alrik.zachert@unjudder.com"
        }
    ],
    "require": {
        "php": ">=5.3.3.",
        "ext-intl": "*",
        "zendframework/zend-eventmanager": "2.*",
        "zendframework/zend-http": "2.*",
        "zendframework/zend-modulemanager": "2.*",
        "zendframework/zend-stdlib": "2.*"
    },
    "autoload": {
        "psr-0": {
            "SlmLocale": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
